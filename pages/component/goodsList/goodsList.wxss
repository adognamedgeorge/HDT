/* pages/component/goodsList/goodsList.wxss */
.tabbar, .content {
  display: flex;
  justify-content: space-around;
  align-items: center;
  padding: 30rpx;
}

.content_lf {
  width: 200rpx;
  height: 200rpx;
  border-radius: 30rpx;
  overflow: hidden;
  margin-right: 20rpx;
}

.lf_img {
  width: 100%;
  height: 100%;
}

.rt_top {
  max-height: 100rpx;
  line-height: 50rpx;
  overflow: hidden;
  font-size: 34rpx;
}

.rt_mid {
  font-size: 20rpx;
  color: lightgrey;
}

.price {
  color: red;
}

.del_price {
  text-decoration: line-through;
  color: grey;
}

.btm_img {
  float: right;
  width: 50rpx;
  height: 50rpx;
}