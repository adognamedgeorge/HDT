<view class="page">
  <view class="my_cells">
    <mp-cells>
      <mp-cell title="运单号" footer="37948798475"></mp-cell>
      <mp-cell title="物流公司" footer="中通快递"></mp-cell>
    </mp-cells>
  </view>

  <view class="imgs">
    <view class="imgs_box">
      <image class="my_img" src="../../../../../../resources/img/a.png" />
      <image class="my_img" src="../../../../../../resources/img/a.png" />
      <image class="my_img" src="../../../../../../resources/img/a.png" />
    </view>
  </view>

  <view class="slider_program">
    <slider class="my_slider"></slider>

    <view class="log_program">
      <view class="width_limit" wx:for="{{logDetailData}}" wx:key="index">
        <view class="{{index == 0 ? 'navActive' : ''}}">您的包裹已到「{{item.add}}」已到达</view>
        <text class="log_txt">{{item.date}}</text>
      </view>
    </view>
  </view>
</view>
