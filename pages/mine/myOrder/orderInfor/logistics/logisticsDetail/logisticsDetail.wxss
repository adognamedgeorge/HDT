/* pages/mine/myOrder/orderInfor/logistics/logisticsDetail/logisticsDetail.wxss */
.page {
  font-size: 28rpx;
  color: #333333;
}

.my_cells {
  font-size: 28rpx;
}

.weui-cells:after {
  border-bottom: 1px dashed #F6F6F7;
}


.imgs {
  overflow-x: scroll;
  height: 160rpx;
  margin: 41rpx auto 0 auto;
  box-shadow: 0 4px 15px 0 rgba(108,130,190,0.16);
  border-radius: 8rpx;
  padding: 26rpx 0;
  width: 90%;
  margin-bottom: 80rpx;
}

.imgs_box {
  align-items: center;
  display: flex;
  flex-wrap: nowrap;
  justify-content: flex-start;
  width: 100%;
}

.my_img {
  width: 160rpx;
  height: 160rpx;
  margin: 0 45rpx;
}

.slider_program {
  background: white;
  margin-top: 23rpx;
}

.log_program {
  width: 80%;
  margin: 0 auto;
}

.width_limit {
  padding-bottom: 80rpx;
  padding-left: 100rpx;
  position: relative;
  border-left: 1px solid #CCCCCC;
}

.no_border {
  border-left: none;
}

.log_txt {
  color: #888888;
}

.navActive {
  color: #E91010;
  font-size: 34rpx;
}